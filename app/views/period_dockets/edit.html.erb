<div class='breadcrumbs'>
  <ul>
    <li>
      <%= link_to 'Список периодов', periods_path %>
    </li>
    <li>
      <%= link_to @period, period_path(@period) %>
    </li>
    <li>
      <%= link_to "гр. #{@group}", period_group_path(@period, @group) %>
    </li>
  </ul>
</div>
<div class="actions">
  <%= link_to "Удалить ведомость",
    period_group_docket_path(@period, @group, @docket), :method => :delete,
    :confirm => "Уверены что хотите удалить ведомость?" if can? :destoy, @docket %>
</div>
<%= render 'form' %>
